
import java.util.Scanner;

public class entrada {
	public static void main(String a[]){
		Scanner X= new Scanner(System.in);
		int r= X.nextInt();
		int s= X.nextInt();
		int t=r+s;
		System.out.println("el valor digitado A es:"+r);
		System.out.println("el valor digitado B es:"+s);
		System.out.println("el resultado de la suma es:"+t);
        r=s;
        t=r;
        s=t;
        System.out.println("el valor digitado de r es:"+s);
        System.out.println("el valor digitado de s es:"+r);
        
       if(r>s)
    	   System.out.println(r+"elevarlo a s"); 
       
     }

   }


